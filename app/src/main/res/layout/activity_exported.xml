<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ExportedActivity">
    <TextView
        android:id="@+id/exported_activity_info_text"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        android:text="@string/exported_activity_text" />

    <TextView
        android:layout_marginTop="20dp"
        android:text="传入的数据为"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/exported_activity_vul_text"
        app:layout_constraintTop_toBottomOf="@id/exported_activity_info_text" />


    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        android:text="@string/back_btn"
        android:id="@+id/back_btn"
        android:layout_marginBottom="100dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>